#version 330 core

// Interpolated values from the vertex shaders
in vec2 UV;

// Ouput data
out vec3 color;

// Values that stay constant for the whole mesh.
uniform sampler2D textureSamplerSky;

void main() {
	vec3 col3 = texture(textureSamplerSky, UV).rgb;
	color = vec3(col3[0], col3[1], col3[2]);
}
